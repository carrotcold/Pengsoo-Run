{
  "name": "pengsoo-run",
  "version": "1.0.0",
  "description": "Avoids UnhandledPromiseRejectionWarning and PromiseRejectionHandledWarning",
  "repository": "https://github.com/pengsoo-run/pengsoo-run.git",
  "author": "Soin Na <carrotcold@gmail.com>",
  "scripts": {
    "start": "parcel --https src/index.html -p 3000 --open",
    "build": "parcel build src/index.html --out-dir dist",
    "lint": "eslint src/**/*.{ts,tsx}",
    "lint:fix": "eslint --fix src/**/*.{ts,tsx}"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.5.0",
    "phaser": "^3.24.1",
    "qrcode.react": "^1.0.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-redux": "^7.2.2",
    "react-router-dom": "^5.2.0",
    "redux": "^4.0.5",
    "socket.io-client": "^2.3.1",
    "styled-components": "^5.2.1",
    "styled-normalize": "^8.0.7"
  },
  "devDependencies": {
    "@types/node": "^14.14.13",
    "@types/qrcode.react": "^1.0.1",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-redux": "^7.1.12",
    "@types/react-router-dom": "^5.1.6",
    "@types/socket.io-client": "^1.4.34",
    "@types/styled-components": "^5.1.5",
    "@typescript-eslint/eslint-plugin": "^4.9.0",
    "@typescript-eslint/parser": "^4.9.0",
    "eslint": "^7.15.0",
    "parcel-bundler": "^1.12.4",
    "parcel-plugin-clean-easy": "^1.0.2",
    "parcel-plugin-static-files-copy": "^2.5.0",
    "typescript": "^4.1.2"
  },
  "staticFiles": {
    "staticPath": "assets",
    "watcherGlob": "**"
  },
  "parcelCleanPaths": [
    "dist"
  ]
